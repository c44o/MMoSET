###  http://www.semanticweb.org/admin/ontologies/2025/9/task2#hasAncestor
:hasAncestor rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :hasAncestor ;
             owl:inverseOf :hasDescendant ;
             rdf:type owl:TransitiveProperty .


###  http://www.semanticweb.org/admin/ontologies/2025/9/task2#hasChild
:hasChild rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :hasDescendant ;
          owl:inverseOf :hasParent .


###  http://www.semanticweb.org/admin/ontologies/2025/9/task2#hasDescendant
:hasDescendant rdf:type owl:ObjectProperty ,
                        owl:TransitiveProperty .


###  http://www.semanticweb.org/admin/ontologies/2025/9/task2#hasParent
:hasParent rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf :hasAncestor ;
           rdf:type owl:FunctionalProperty ,
                    owl:AsymmetricProperty ,
                    owl:IrreflexiveProperty .


